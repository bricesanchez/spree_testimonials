<%= render :partial => 'spree/admin/shared/configuration_menu' %>

<h1><%= t("editing_testimonial") %></h1>
<%= render "spree/shared/error_messages", :target => @testimonial %>

<%=form_for(:testimonial, :url => admin_testimonial_url(@testimonial), :method => :put) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>

  <p class="form-buttons">
    <%= button t("actions.update"), nil, 'submit', {"class" => "wymupdate"} %>
    <%= t("or") %> <%= link_to t("actions.cancel"), collection_url %>
  </p>
<% end %>
