<%= render :partial => 'spree/admin/shared/configuration_menu' %>

<h1><%= t("new_testimonial") %></h1>
<%= render "spree/shared/error_messages", :target => @testimonial %>

<% form_for(:testimonial, :url => collection_url) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p class="form-buttons">
    <%= button t("actions.create"), nil, 'submit', {"class" => "wymupdate"} %>
    <%= t("or") %> <%= link_to t("actions.cancel"), collection_url %>
  </p>
<% end %>
